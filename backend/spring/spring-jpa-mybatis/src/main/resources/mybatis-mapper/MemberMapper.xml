<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.engkimbs.springjpamybatis.member.query.mapper.MemberMapper">

    <select id="getMember" resultType="MemberModel">
        select name, point from member where id = #{id}
    </select>

    <select id="getAllMemberList" resultType="MemberModel">
        select name, point from member
    </select>

</mapper>